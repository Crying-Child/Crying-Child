spawn(function()
local part = tab2[i]
local partbp = tab[i]
local partl = tab3[i]
partbp.D = 150
local arm = arms[i]
repeat game['Run Service'].Heartbeat:wait()
    cf = arm.CFrame*CFrame.new(0,0,-1)*CFrame.Angles(math.rad(1),math.rad(1),math.rad(1))
partbp.Position = cf.p
partl.CFrame = cf

until _G.nobone

part.CFrame = CFrame.new(0,-1000,0)
wait()
part:Destroy()
end)
end
end
end)
wait(2)
detect:Disconnect()
val:Destroy()

end

